<p id="notice"><%= notice %></p>

<%= link_to 'Update', edit_result_path(@result) %> |
<%= link_to 'Back', results_path %>

<div class="header"><h1><%= @result.name %></h1></div>

<div class="data">
<p>
  <strong>Emis:</strong>
  <%= @result.emis %>
</p>

<p>
  <strong>Centre no:</strong>
  <%= @result.centre_no %>
</p>

<p>
  <strong>Name:</strong>
  <%= @result.name %>
</p>

<p>
  <strong>Wrote 2014:</strong>
  <%= @result.wrote_2014 %>
</p>

<p>
  <strong>Passed 2014:</strong>
  <%= @result.passed_2014 %>
</p>

<p>
  <strong>Wrote 2015:</strong>
  <%= @result.wrote_2015 %>
</p>

<p>
  <strong>Passed 2015:</strong>
  <%= @result.passed_2015 %>
</p>

<p>
  <strong>Wrote 2016:</strong>
  <%= @result.wrote_2016 %>
</p>

<p>
  <strong>Passed 2016:</strong>
  <%= @result.passed_2016 %>
</p>
</div>


<html>
  <head>
    <script type="text/javascript" src="loader.js"></script>
    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Year', 'Pass Rate in Percentage'],
          ['2014', <%= @pass.passrate2014 %>],
          ['2015', <%= @pass.passrate2015 %>],
          ['2016', <%= @pass.passrate2016 %>],

        ]);
           
        var options = {
          title:'Matric Pass Rate From 2014 To 2016 ',
          width:1200,
          height:800,
          pieStartAngle: 100,
          pieSliceText: 'value',
          backgroundColor: '#c0d1ff',
          is3D: true};

        var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
        chart.draw(data, options);
      }
    </script>
  </head>

  <body>
    <div id="chart_div"></div>
  </body>
</html>


